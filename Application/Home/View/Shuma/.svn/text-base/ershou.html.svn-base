	var oUl = obj.getElementsByTagName("ul")[0];
	var aLi = oUl.getElementsByTagName("li");
	var index = 1;
	var Timer = null;
	oUl.style.width = aLi[0].offsetWidth * aLi.length + "px";
	ofan_l.onclick=function ()
	{
		index += 1;
		if(index == 4){
			index = 1;
		}
		show();
	};
	ofan_r.onclick=function ()
	{
		index -= 1;
		if(index == 0){
			index = 3;
		}
		show();
	};
	
	oUl.onmouseover = function (){clearInterval(Timer)};
	oUl.onmouseout = function ()
	{
		clearInterval(Timer);
		Timer =setInterval(function ()
		{
			setTimer();
		},10000);
	};
	Timer = setInterval(function (){
		setTimer();
	},10000);
	function setTimer()
	{
		index += 1;
		if(index == 4)
		{
			index = 1;
		}
		show();
	}
	function show()
	{
		var goal = (-aLi[0].offsetWidth * 4) * (index-1);
		opl20.innerHTML = index;
		startMove(oUl, {"left": goal});
	}
})();

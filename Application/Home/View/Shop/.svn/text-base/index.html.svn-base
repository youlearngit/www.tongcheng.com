                    foldObj.attr('class','sq_m').text(fold).show();
                };
            }
            foldObj.click(function(){
                var tag = $(this).attr('class');
                if(tag == 'sq_m'){
                    dd.addClass('autoHeight');
                    foldObj.attr('class','s_sq').text(unfold).show();
                    foldObj.append("<i class='sq_s'>");
                }else{
                    dd.removeClass('autoHeight');
                    foldObj.attr('class','sq_m').text(fold).show();
                }
                return false;
            })

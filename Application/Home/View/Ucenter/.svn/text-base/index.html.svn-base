                error: function(){
                    alert('显示失败');
                    $('#phone_alias_button_' + infoId).html(html);
                },
                success: function(json) {
                    if(json && json.code == 0) {
                        $('#pao_container_' + infoId).html('小号：' + alias);
                        $('#phone_alias_button_' + infoId).fadeOut();
                    }else {
                        alert('显示失败:' + json.msg);
                        $('#phone_alias_button_' + infoId).html(html);
                    }
                }
            });
        }
       

      
       

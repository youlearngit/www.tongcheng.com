                <script type="text/javascript">
                //获取元素
                $('.slides div').css('display','none');
                $('.slides div').eq(0).css('display','block');

                //绑定鼠标移入事件
                var index = 0
                var inte = null;

                $('.slides_btn span').mouseover(function(){
                    $(this).attr('class','on');
                    $(this).siblings().removeAttr('class','on');
                    index = $(this).index();
                    $('.slides div').eq(index).show();
                    $('.slides div').eq(index).siblings().css('display','none');
                    clearInterval(inte);
                })

                //绑定鼠标移开事件
                $('.slides_btn span').mouseout(function(){
                    $(this).attr('class','on');
                    inte = setInterval(function(){
                        $('.slides div').eq(index).show();
                        $('.slides div').eq(index).siblings().css('display','none');
                        $(this).attr('class');
                        $(this).siblings().removeAttr('class','on');
                        index++;
                        if(index>1){
                            index = 0;
                        }
                    },3000);
                })


                 inte = setInterval(function(){

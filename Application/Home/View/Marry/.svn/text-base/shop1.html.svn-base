<script type="text/javascript">
$(function () {
	var liArr = new Array("index","policy","support","news","products","toapply");
	var url = location.href;
	for (var i=0; i<liArr.length; i++) {
		if (url.indexOf(liArr[i]) > -1) {
			$("#navBack").find("li").each(function(index){
				$(this).removeClass("active");
			});
			$("#" + liArr[i]).addClass("active");
		}
	}
});
</script>

